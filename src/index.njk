{% import 'macros/triangle.njk' as tri %}

<section id="section-start">
  <svg
    class="tri"
    w:h="max-[30rem]"
    viewBox="0 0 1000 866"
    preservAspectRatio="xMidYMid"
  >
    {{ tri.right(8) }}
  </svg>
  <header w:grid="row-start-2 col-start-1">
    <div class="logo">
      <h1>
        designexit <br/>
        <span>2022</span>
