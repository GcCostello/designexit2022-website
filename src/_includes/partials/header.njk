<header
      data-current="{{'student' if '/student/' in page.url else ''}}"
      class="flex justify-between py-2 border-b-2 border-black">
  <button w:m="x-[200px]" onclick="
      document.documentElement.classList.toggle('dark')
      ">dark</button>
  <nav
        id="site-nav"
        class="flex w-full">
    <button aria-label="navigera" class="hamburger ml-auto relative z-50" w:pointer="auto">
      <svg
            aria-hidden="true"
            class=""
            width="2.5rem"
            height="2rem"
            viewBox="0 0 48 32"
            preservAspectRatio="xMidYMid">
        <style>
          .hamburger line {
            transition-property: transform, opacity;
            transition-duration: 50ms, 25ms;
          }
          [data-menu-state="open"] .hamburger line:first-child {
            transform: translate(8px, -1.5px) rotate(45deg);
          }
          [data-menu-state="open"] .hamburger line:nth-child(2) {
            transform-origin: center;
            transform: rotate(45deg);
            opacity: 0;
          }
          [data-menu-state="open"] .hamburger line:last-child {
            transform-origin: 0 31px;
            transform: translate(8px, 1.5px) rotate(-45deg);
          }
        </style>
        <g stroke="hsl(var(--accent))" stroke-width="4">
          <line x1="0" x2="48" y1="1" y2="1"/>
          <line x1="0" x2="48" y1="16" y2="16"/>
          <line x1="0" x2="48" y1="31" y2="31"/>
        </g>
      </svg>
    </button>
    <ol  w:opacity="0" w:bg="primary" class=" nav | fixed z-20 left-0 top-[-100%] w-full h-full flex flex-col items-center justify-center text-xl">
      <li class="pointer-events-auto">
        <a href="/#start">start</a>
      </li>
      <li class="pointer-events-auto">
        <a href="/#designexit">designexit</a>
      </li>
      <li class="pointer-events-auto">
        <a href="/#vilka-ar-vi">vilka Ã¤r vi</a>
      </li>
    </ol>
  </nav>
</header>