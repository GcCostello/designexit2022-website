<header w:p="b-2xs" w:flex="~" w:align="items-center" w:border="b-2 accent">
  <div w:display="hidden" class="logo">LOGO</div>
  <nav w:m="l-auto" id="site-nav">
    <button
      w:z="100"
      onclick="
    const parent = event.target.parentElement
    const state = parent.dataset.menuState
    
    state ? parent.dataset.menuState = '' : parent.dataset.menuState = 'open'
    "
    >
      <svg
        width="2.5rem"
        height="2.5rem"
        viewBox="0 0 32 32"
        preservAspectRatio="xMidYMid"
        style="pointer-events: none"
      >
        <g stroke="black" stroke-width="2">
          <line x1="0" x2="100%" y1="10%" y2="10%" />
          <line x1="0" x2="100%" y1="50%" y2="50%" />
          <line x1="0" x2="100%" y1="90%" y2="90%" />
        </g>
      </svg>
    </button>
    <ol w:text="truncate">
      <li>
        <a href="">Start</a>
      </li>
      <li>
        <a href="">Vad är Designexit</a>
      </li>
      <li>
        <a href="">Vilka är vi</a>
      </li>
      <li>
        <a href="">Vernisage</a>
      </li>
    </ol>
  </nav>
</header>
